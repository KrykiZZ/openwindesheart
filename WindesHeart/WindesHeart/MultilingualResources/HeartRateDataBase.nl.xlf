        </trans-unit>
        <trans-unit id="Main_Fetch" translate="yes" xml:space="preserve">
          <source>Fetch data</source>
          <target state="new">Fetch data</target>
        </trans-unit>
        <trans-unit id="Main_No_Device" translate="yes" xml:space="preserve">
          <source>No device paired</source>
          <target state="new">No device paired</target>
        </trans-unit>
        <trans-unit id="Main_Pair" translate="yes" xml:space="preserve">
          <source>Tap to pair device</source>
          <target state="new">Tap to pair device</target>
        </trans-unit>
        <trans-unit id="Main_Fetching" translate="yes" xml:space="preserve">
          <source>Fetching...</source>
          <target state="new">Fetching...</target>
        </trans-unit>
        <trans-unit id="Main_Location_Text" translate="yes" xml:space="preserve">
          <source>Bluetooth need access the location service to find nearby devices. Your location data will not be used.</source>
          <target state="new">Bluetooth need access the location service to find nearby devices. Your location data will not be used.</target>
        </trans-unit>
        <trans-unit id="Main_Location_Title" translate="yes" xml:space="preserve">
          <source>Location</source>
          <target state="new">Location</target>
        </trans-unit>
        <trans-unit id="Main_Ok" translate="yes" xml:space="preserve">
          <source>Ok</source>
          <target state="new">Ok</target>
        </trans-unit>
      </group>
